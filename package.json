{
  "name": "react-storybook-design-system",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.5.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "polished": "4.1.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-redux": "7.2.2",
    "react-scripts": "4.0.0",
    "react-spring": "8.0.27",
    "styled-components": "5.2.1",
    "styled-components-modifiers": "1.2.5"
  },
  "scripts": {
    "eslint-check": "npx eslint-config-prettier src/components/button/index.js",
    "lint": "npm-run-all --parallel --print-label --continue-on-error lint:*",
    "lint:css": "stylelint 'src/**/*.js'",
    "lint:js": "eslint --ext js --ext mjs --max-warnings 0 .storybook/ src/",
    "start": "react-scripts start",
    "build": "EXTEND_ESLINT=true react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook --static-dir public"
  },
  "browserslist": {
    "production": [
      "last 2 versions",
      "not dead",
      "not ie < 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-a11y": "6.1.21",
    "@storybook/addon-essentials": "6.1.21",
    "@storybook/addon-links": "6.1.21",
    "@storybook/preset-create-react-app": "3.1.6",
    "@storybook/react": "6.1.21",
    "babel-eslint": "10.1.0",
    "eslint": "7.21.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "react-test-renderer": "^17.0.1",
    "stylelint": "13.11.0",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-styled-components": "0.1.1"
  }
}
